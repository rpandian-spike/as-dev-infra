{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            //on container run: python3 -m debugpy --listen 0.0.0.0:4001 --wait-for-client ./examples/client/putget_async.py
            "name": "Python: debugpy Attach",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 4001
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "/code/src/aerospike/enterprise/"
                }
            ]
        },
        {
            "name": "C++ SSH Attach in as-server",
            "type": "cppdbg",
            "request": "attach",
            "program": "/code/src/aerospike/enterprise/aerospike-server/target/Linux-x86_64/bin/asd",
            "processId": "${command:pickRemoteProcess}",
            "pipeTransport": {
                "debuggerPath": "/usr/bin/gdb",
                "pipeProgram": "/usr/bin/ssh",
                "externalConsole": false,
                "pipeArgs": [
                    "root@127.0.0.1",
                    "-p",
                    "2222"
                ],
                "pipeCwd": ""
            },
            "sourceFileMap": {
                "/code/src/aerospike/enterprise/": "${workspaceRoot}"
            }
        },
        {
            "name": "SSH Launch",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "/code/src/aerospike/enterprise/scan_partition-python/test/new_tests/",
            "program": "python3",
            "args": [
                " -m pytest /code/src/aerospike/enterprise/scan_partition-python/test/new_tests/test_scan_pagination.py"
            ],
            "pipeTransport": {
                "debuggerPath": "/usr/bin/gdb",
                "pipeProgram": "/usr/bin/ssh",
                "externalConsole": true,
                "pipeArgs": [
                    "root@127.0.0.1",
                    "-p",
                    "2225"
                ],
                "pipeCwd": "/usr/bin/"
            },
            "sourceFileMap": {
                "/code/src/aerospike/enterprise/": "/code/src/aerospike/enterprise/"
            }
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "internalConsole",
            "justMyCode": false
        },
        {
            "name": "Python: Attach using Process Id",
            "type": "python",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": false
        },
        {
            "name": "Debug Tests",
            "type": "python",
            "request": "test",
            "console": "internalConsole",
            "justMyCode": false
        },
        {
            "name": "Python: C-Extension (lldb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "/usr/local/bin/python3",
            "processId": "${command:pickProcess}",
            "MIMode": "lldb"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/code/src/aerospike/enterprise/aerospike-server/target/Linux-x86_64/bin/asd",
            "args": [
                "--config-file=/code/src/aerospike/enterprise/runserver/scripts/asd1.conf --cold-start --instance=1"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "C++ Docker ASD Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "/code/src/aerospike/enterprise/aerospike-server/target/Linux-x86_64/bin/asd",
            "processId": "${command:pickRemoteProcess}",
            "pipeTransport": {
                "debuggerPath": "/usr/bin/gdb",
                "pipeCwd": "${workspaceRoot}",
                "pipeProgram": "docker",
                "pipeArgs": [
                    "exec",
                    "-i",
                    "as-docker-cluster_as-server-node1_1", //"docker-dev_as-server-node3_1",
                    "sh",
                    "-c"
                ],
                "cwd": "${workspaceFolder}",
                "environment": [],
                "externalConsole": true,
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ]
            },
            "sourceFileMap": {
                "/code/src/aerospike/enterprise/": "${workspaceRoot}"
            }
        },
        {
            "name": "C++ Docker Launch",
            "type": "cppdbg",
            "request": "launch",

            // "cwd": "/code/src/aerospike/enterprise/aerospike-client-c/examples/async_examples/async_get/target/",
            // "program": "./example",
            // "args": [
            //     "-has-s1.as-network.com",
            //     "-Uram",
            //     "-Pram"
            // ],

            // "cwd": "/code/src/aerospike/enterprise/aerospike-client-python/test/new_tests/",
            // "program": "python3",
            // "args": [
            //     "-mpytest",
            //     "./test_scan_pagination.py",
            //     "-ktest_scan_pagination_with_existent_ns_and_set"
            // ],

            // "cwd": "/code/src/aerospike/enterprise/aerospike-client-python/examples/client",
            // "program": "python3",
            // "args": [
            //     "./scan_partition.py",
            //     "-has-s1.as-network.com",
            //     "-Uadmin",
            //     "-Padmin",
            //     "-i1000"
            // ],

            //-exec source /code/src/aerospike/enterprise/cpython-39/build/python.exe-gdb.py
            //-exec py-bt
            //-exec bt
            "cwd": "/code/src/aerospike/enterprise/rp-async/",
            "program": "/usr/local/bin/python3.9d",
            "args": [
                "./examples/client/get_async.py",
                "-c1"
            ],

            "pipeTransport": {
                "debuggerPath": "/usr/bin/gdb",
                "pipeCwd": "/usr/bin/",
                "pipeProgram": "docker",
                "pipeArgs": [
                    "exec",
                    "-i",
                    "as-docker-cluster_as-client-node1_1",
                    "bash",
                    "-c"
                ],
                "cwd": "${workspaceFolder}",
                "environment": [],
                "externalConsole": "integratedConsole",
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ]
            },
            "sourceFileMap": {
                "/code/src/aerospike/enterprise/": "${workspaceRoot}",
                //"/code/src/aerospike/enterprise/aerospike-client-c/": "${workspaceRoot}/rp-async/aerospike-client-c",
                "/code/src/": "${workspaceRoot}/../../"
            }
        }
    ]
}